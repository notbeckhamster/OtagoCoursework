    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) No args",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Skeleton",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Skeleton",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
